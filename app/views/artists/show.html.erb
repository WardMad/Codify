<div class="panel panel-primary">

<h3> <%=@artist.name%></h3>

<h4> Age:<%= @artist.age %> </h4>
<h4> Country: <%= @artist.country  %></h4>
<h4> Style: <%= @artist.style %></h4>
<h4>  <%= link_to "More info",  href="https://ru.wikipedia.org/wiki/%D0%91%D0%BE%D1%87%D0%B5%D0%BB%D0%BB%D0%B8,_%D0%90%D0%BD%D0%B4%D1%80%D0%B5%D0%B0", :target => "_blank"%></h4>

<p> Hits: <%= @artist.songs.each do |song| %> </p>
</div>
<% end %>
<div class="img-circle" alt="Cinque Terre">
<% @artist.photos.each do |photo| %>
  <%= image_tag photo.image %>
</div>
  <%end%>
