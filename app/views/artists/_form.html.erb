<%= form_for @artist do |f| %>

<div class="form-group">
<label>Name</label>
<%= f.text_field :name, placeholder: "Name", class: "form-control" %>
</div>

<div class="form-group">
<label>Age</label>
<%= f.text_field :age, placeholder: "Age", class: "form-control" %>
</div>

<div class="form-group">
<label>Country</label>
<%= f.text_field :country, placeholder: "Country", class: "form-control" %>
</div>

<div class="form-group">
<label>Style</label>
<%= f.text_field :style, placeholder: "Style", class: "form-control" %>
</div>

<div class="actions">

  <%= link_to "Back", root_path, class: "btn btn-default" %>
</div>
<% end %>
